package Iteration;

import java.util.ArrayList;
import java.util.List;

public class IterationMethods {
	
	public ArrayList<Integer> getOdd(ArrayList<Integer> nums) {
		ArrayList<Integer> numsNew = new ArrayList<>();
		for (int i : nums) {
			if (i % 2 == 0) {
				numsNew.add(i);
			}
		}
		return numsNew;
	}

}
