package gar;

public class Unicycle extends Vehicle{
	
	private int noOfPedals;
	private int defaultNoOfPedals;
	
	public int getNoOfPedals() {
		return noOfPedals;
	}
	public void setNoOfPedals(int noOfPedals) {
		this.noOfPedals = noOfPedals;
	}
	public int getDefaultNoOfPedals() {
		return defaultNoOfPedals;
	}
	public void setDefaultNoOfPedals(int defaultNoOfPedals) {
		this.defaultNoOfPedals = defaultNoOfPedals;
	}

	
	
	

}
