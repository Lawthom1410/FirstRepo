package ExRes;

public class ExamResultsApp {

	public static void main(String[] args) {
		
		int physics = 100;	
		int chemistry = 50;
		int biology = 105;
		int total = physics + chemistry + biology;
		float percentage = (total/450)*100;
		
		ExamResults res = new ExamResults();
		res.displayRes(physics, chemistry);

	}

}
